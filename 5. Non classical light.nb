Cell[TextData[{
 "The unitary squeezed operator is defined as S(\[Xi]) = ",
 Cell[BoxData[
  FormBox[
   RowBox[{Cell["exp",ExpressionUUID->"4817adaa-ac76-b140-866d-49dd9e8e71bb"], 
    RowBox[{"(", 
     RowBox[{
      RowBox[{
       RowBox[{"1", "/", "2"}], " ", 
       SuperscriptBox["\[Xi]", "*"], 
       SuperscriptBox["a", "2"]}], "-", 
      RowBox[{
       RowBox[{"1", "/", "2"}], " ", "\[Xi]", " ", 
       SuperscriptBox["a", "\[Dagger]2"]}]}], ")"}]}], TraditionalForm]],
  ExpressionUUID->"0d08bf85-6683-984f-922d-0f0d337361d3"],
 " can be understood as a two photon generalization of displacement operator, \
where \[Xi] is a complex number \[Xi] = r ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["e", 
    RowBox[{"\[ImaginaryI]", " ", "\[Theta]"}]], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "861e3199-795e-974c-830b-ae9e85d73a45"],
 ", r is called the squeeze parameter ",
 Cell[BoxData[
  FormBox[
   RowBox[{
   "0", " ", "\[LessEqual]", " ", "r", " ", "\[LessEqual]", " ", 
    "\[Infinity]"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "4f6cead6-aeba-e945-85d9-18bc44b32831"]
}], "Text",
 CellChangeTimes->{{3.925569307952734*^9, 3.925569492734649*^9}, {
  3.9309090392363834`*^9, 3.9309090525470505`*^9}, {3.93091591297278*^9, 
  3.9309159675353413`*^9}},ExpressionUUID->"5e777593-2099-4742-af31-\
adddc82da2e0"],

Cell[BoxData[
 RowBox[{
  RowBox[{"squeeze", "[", 
   RowBox[{"\[Xi]_", "?", "NumericQ"}], "]"}], ":=", " ", 
  RowBox[{"Exp", "[", 
   RowBox[{"0.5", 
    RowBox[{"(", " ", 
     RowBox[{
      RowBox[{
       RowBox[{"Conjugate", "[", "\[Xi]", "]"}], 
       RowBox[{"(", 
        RowBox[{"a", "@", "a"}], ")"}]}], "-", 
      RowBox[{"\[Xi]", " ", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"a", "[", "\"\<Dagger\>\"", "]"}], "@", 
         RowBox[{"a", "[", "\"\<Dagger\>\"", "]"}]}], ")"}]}]}], ")"}]}], 
   "]"}]}]], "Input",
