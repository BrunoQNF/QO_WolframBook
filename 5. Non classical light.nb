     SubscriptBox["n", "4"], " ", "is", " ", "related", " ", "to", " ", "the",
      " ", "photocurrent", " ", "difference", " ", "which", " ", "is", " ", 
     "an", " ", "observable", " ", "with", " ", "direct", " ", "experimental",
      " ", "interpretation"}]}], TraditionalForm]],
  FontSize->18,ExpressionUUID->"3fef0a08-bbc6-6742-b763-5e741624c376"]
}], "Text",
