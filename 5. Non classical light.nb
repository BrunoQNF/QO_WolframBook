Cell[TextData[{
 "Squeezed states are called ",
 StyleBox["non-classical",
  FontWeight->"Bold"],
 " because using the ",
 StyleBox["Glauber",
  IgnoreSpellCheck->True],
 "-",
 StyleBox["Sudarshan P function it can be showed if one quadrature variance \
is < 1/4 implies P(\[Alpha]) is non negative at least in some phase space \
region. It is also useful to introduce a generic quadrature operator ",
  IgnoreSpellCheck->True],
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     OverscriptBox["X", "^"], "(", "\[Theta]", ")"}], " ", "=", " ", 
    RowBox[{
     RowBox[{"1", "/", "2"}], 
     RowBox[{"(", 
      RowBox[{
       RowBox[{
        OverscriptBox["a", "^"], "  ", 
        SuperscriptBox["e", 
         RowBox[{
          RowBox[{"-", "\[ImaginaryI]"}], " ", "\[Theta]"}]]}], "+", 
       RowBox[{
        SuperscriptBox[
         OverscriptBox["a", "^"], "\[Dagger]"], " ", 
        SuperscriptBox["e", 
         RowBox[{"\[ImaginaryI]", " ", "\[Theta]"}]]}]}], ")"}]}]}], 
   TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "a48acbc2-b048-0f4b-b279-2e06206db166"]
}], "Text",
