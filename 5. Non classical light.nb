Cell[TextData[{
 StyleBox["Using a balanced BS it can be proved :   ",
  FontSize->18],
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["i", "34"], "=", " "}], TraditionalForm]],ExpressionUUID->
  "664013bc-5671-dc47-81b8-c96faf65d11c"],
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["n", "3"], "-", 
     SubscriptBox["n", "4"]}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{
       SuperscriptBox[
        SubscriptBox["a", "3"], "\[Dagger]"], 
       SubscriptBox["a", "3"]}], "-", 
      RowBox[{
       SuperscriptBox[
        SubscriptBox["a", "4"], "\[Dagger]"], 
       SubscriptBox["a", "4"]}]}], "=", 
     RowBox[{"-", 
      RowBox[{"2", "[", 
       RowBox[{
        FractionBox["1", 
         RowBox[{"2", " ", "\[ImaginaryI]"}]], 
        RowBox[{"(", 
         RowBox[{
          RowBox[{
           SuperscriptBox[
            SubscriptBox["a", "2"], "\[Dagger]"], 
           SubscriptBox["a", "1"]}], "-", 
          RowBox[{
           SuperscriptBox[
            SubscriptBox["a", "1"], "\[Dagger]"], 
           SubscriptBox["a", "2"]}]}], ")"}]}], "]"}]}]}]}], TraditionalForm]],
  FontSize->18,ExpressionUUID->"ece4c166-8c83-9849-a4c3-a9ba5aacd02f"],
 StyleBox["\n",
  FontSize->18],
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["n", "3"], "-", 
